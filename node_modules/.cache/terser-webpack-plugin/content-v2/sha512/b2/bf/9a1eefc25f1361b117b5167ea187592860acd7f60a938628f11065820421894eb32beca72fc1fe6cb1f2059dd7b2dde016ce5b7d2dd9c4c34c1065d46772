. \")]),s(\"span\",[a._v(\"Ejemplo estructura de una Clase\")])])},function(){var a=this,t=a.$createElement,e=a._self._c||t;return e(\"div\",{staticClass:\"row mb-5 justify-content-center\"},[e(\"div\",{staticClass:\"col-lg-7\"},[e(\"figure\",[e(\"img\",{attrs:{src:s(\"a0b3\"),\"data-aos\":\"zoom-in\"}})])])])},function(){var a=this,t=a.$createElement,e=a._self._c||t;return e(\"div\",{staticClass:\"bg-full-width bg-color-2 mb-5\"},[e(\"div\",{staticClass:\"px-4 px-md-5 py-4\"},[e(\"div\",{staticClass:\"row align-items-center\"},[e(\"div\",{staticClass:\"col-lg-auto\"},[e(\"img\",{staticClass:\"mx-auto\",staticStyle:{\"max-width\":\"90px\"},attrs:{src:s(\"250b\")}})]),e(\"div\",{staticClass:\"col-lg\",attrs:{\"data-aos\":\"fade-right\"}},[e(\"p\",{staticClass:\"mb-0\"},[e(\"b\",[a._v(\"Relaciones:\")]),a._v(\" las relaciones conectan clases para modelar interacciones y dependencias dentro del sistema.\")])])])])])},function(){var a=this,t=a.$createElement,e=a._self._c||t;return e(\"div\",{staticClass:\"col-md-8 col-lg-4 order-lg-2\"},[e(\"figure\",[e(\"img\",{staticClass:\"mb-4 mb-lg-0\",attrs:{src:s(\"2aad\"),alt:\"\",\"data-aos\":\"zoom-in\"}})])])},function(){var a=this,t=a.$createElement,e=a._self._c||t;return e(\"div\",{staticClass:\"bg-full-width bg-color-2\"},[e(\"div\",{staticClass:\"px-4 px-md-5 py-4\"},[e(\"div\",{staticClass:\"row align-items-center\"},[e(\"div\",{staticClass:\"col-lg-auto\"},[e(\"img\",{staticClass:\"mx-auto\",staticStyle:{\"max-width\":\"90px\"},attrs:{src:s(\"fefa\")}})]),e(\"div\",{staticClass:\"col-lg\",attrs:{\"data-aos\":\"fade-right\"}},[e(\"p\",{staticClass:\"mb-0\"},[e(\"b\",[a._v(\"Visibilidad:\")]),a._v(\" define el nivel de acceso a los atributos y métodos de una clase.\")])])])])])},function(){var a=this,t=a.$createElement,e=a._self._c||t;return e(\"div\",{staticClass:\"row justify-content-center align-items-center bg-h85-3\"},[e(\"div\",{staticClass:\"col-lg-12\"},[e(\"div\",{staticClass:\"row justify-content-center align-it